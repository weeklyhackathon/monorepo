{
  "name": "@weeklyhackathon/root",
  "version": "0.0.0",
  "license": "MIT",
  "engines": {
    "node": "22.13.1"
  },
  "type": "module",
  "scripts": {
    "config": "npm ci && cp .env.example .env && npx dotenv -e .env -- npm run prisma:migrate",
    "postinstall": "npm run prisma:generate",
    "typecheck": "npm run typecheck -w=apps/",
    "build": "npm run prisma:generate && npm run build -w=apps/$APP_NAME",
    "start:prod": "npm run start:prod -w=apps/$APP_NAME",
    "script": "npx dotenv -e .env -- npx tsx --tsconfig ./tsconfig.json",
    "prisma:studio": "npx prisma studio --schema=packages/db/src/lib/schema.prisma",
    "prisma:reset": "npx prisma migrate reset --schema=packages/db/src/lib/schema.prisma",
    "prisma:migrate": "npx prisma migrate dev --schema=packages/db/src/lib/schema.prisma",
    "prisma:generate": "npx prisma generate --schema=packages/db/src/lib/schema.prisma",
    "prisma:migrate:prod": "npx prisma migrate deploy --schema=packages/db/src/lib/schema.prisma",
    "prisma:format": "npx prisma format --schema=packages/db/src/lib/schema.prisma",
    "start": "npx dotenv -e .env -e packages/agents/.env.local -- npm run start -w=apps/weeklyhackathon-core",
    "test": "npx dotenv -e .env.test.local -- jest --config jest.config.js --verbose",
    "test:ai": "APP_ENV=test GENERATE_TOOL_SCHEMAS=true npx dotenv -e .env.test.local -- jest --config jest.ai.config.js --verbose",
    "clean": "rm -rf ./dist && rm -rf ./packages/*/dist",
    "docker:build:postgres": "docker build -t postgres-with-vector:latest -f docker_images/postgres_with_vector/Dockerfile .",
    "packagegen": "chmod +x scripts/generatePackage.sh && ./scripts/generatePackage.sh"
  },
  "private": true,
  "devDependencies": {
    "@eslint/compat": "^1.1.1",
    "@eslint/js": "^9.8.0",
    "@jest/globals": "^29.7.0",
    "@langchain/anthropic": "^0.3.12",
    "@swc/core": "^1.10.3",
    "@swc/jest": "^0.2.37",
    "@testing-library/react": "^16.1.0",
    "@types/chance": "^1.1.6",
    "@types/jest": "^29.5.12",
    "@types/koa": "~2.13.5",
    "@types/koa__cors": "^5.0.0",
    "@types/koa-bodyparser": "^4.3.12",
    "@types/koa-router": "^7.4.8",
    "@types/luxon": "^3.4.2",
    "@types/node": "^18.16.9",
    "@types/node-cron": "^3.0.11",
    "@types/react": "19.0.2",
    "@types/react-dom": "19.0.2",
    "@typescript-eslint/eslint-plugin": "^8.19.0",
    "babel-jest": "^29.7.0",
    "esbuild": "^0.19.2",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.1",
    "eslint-plugin-playwright": "^1.6.2",
    "eslint-plugin-prettier": "^2.7.0",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "5.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "prettier": "^2.6.2",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "tslib": "^2.3.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.13.0"
  },
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "dependencies": {
    "@coinbase/agentkit": "^0.1.1",
    "@coinbase/cdp-langchain": "^0.0.15",
    "@koa/cors": "^5.0.0",
    "@langchain/core": "^0.3.37",
    "@langchain/langgraph": "^0.2.44",
    "@langchain/openai": "^0.4.2",
    "@prisma/client": "^6.3.0",
    "@typescript-eslint/parser": "^8.19.0",
    "@weeklyhackathon/utils": "^0.0.1",
    "dotenv-cli": "^8.0.0",
    "koa": "~2.14.1",
    "koa-bodyparser": "^4.4.1",
    "koa-router": "^13.0.1",
    "luxon": "^3.5.0",
    "node-cron": "^3.0.3",
    "openai": "^4.77.0",
    "prisma": "^6.3.0",
    "source-map-support": "^0.5.21",
    "tiktoken": "^1.0.20",
    "tsx": "^4.19.2",
    "uuid": "^11.0.3",
    "viem": "^2.22.19"
  },
  "overrides": {
    "@types/react": "19.0.2",
    "@types/react-dom": "19.0.2"
  }
}
